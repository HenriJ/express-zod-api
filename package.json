{
  "private": true,
  "workspaces": [
    "express-zod-api",
    "example",
    "integration-test"
  ],
  "scripts": {
    "start": "yarn workspace example start",
    "build": "yarn workspace express-zod-api build",
    "build:int": "node -r @swc-node/register tools/integration-test.ts && yarn workspace integration-test install",
    "test": "yarn workspace express-zod-api test",
    "test:int": "yarn workspace integration-test jest",
    "test:952": "yarn workspace integration-test tsc -p tsconfig.issue952.json && rm integration-test/issue952.d.ts"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.8",
    "@swc/core": "^1.3.100",
    "@tsconfig/node18": "^18.2.2",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "typescript": "^5.3.2",
    "zod": "^3.22.4"
  }
}
