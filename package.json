{
  "name": "express-zod-api",
  "version": "0.0.1",
  "description": "Express ZOD API",
  "license": "MIT",
  "scripts": {
    "start": "ts-node example/index.ts",
    "cleanup": "rm -rf ./dist",
    "build": "yarn cleanup && tsc",
    "test": "yarn jest --verbose",
    "lint": "yarn eslint ./src ./example ./tests",
    "precommit": "yarn lint && yarn test",
    "install_hooks": "husky install"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "winston": "^3.3.3",
    "zod": "^3.0.0-alpha.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.20",
    "@types/triple-beam": "^1.3.2",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "husky": "^5.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RobinTail/express-zod-api"
  },
  "keywords": [
    "typescript",
    "api",
    "zod",
    "express",
    "http",
    "server",
    "validation",
    "schema",
    "endpoint",
    "handler",
    "json"
  ]
}
